        <pdfEncoding>Identity-H</pdfEncoding><!--这里的定义就与上面1.1的图设置相匹配了-->
        <pdfEmbedded>true</pdfEmbedded>
        <exportFonts>
            <export key="net.sf.jasperreports.html">'SIMLI', Arial, Helvetica, sans-serif</export>
            <export key="net.sf.jasperreports.xhtml">'SIMLI', Arial, Helvetica, sans-serif</export>
